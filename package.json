{
    "private": true,
    
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watchS": "node-sass -o css/index.css resources/sass/index.scss -w",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production"
    },
    "devDependencies": {
        "axios": "^0.27.2",
        "bootstrap": "^4.0.0",
        "cross-env": "^7.0",
        "jquery": "^3.2",
        "laravel-mix": "^6.0.43",
        "lodash": "^4.17.19",
        "node-sass": "^6.0.1",
        "popper.js": "^1.12",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.52.1",
        "sass-loader": "^10.2.1",
        "vue": "^3.2.33",
        "vue-template-compiler": "^2.6.10"
    },
    "watch": {
        "build": {
            "patterns": [
                "src"
            ],
            "extensions": "js,jsx"
        }
    },
    "dependencies": {
        "@serializedowen/vue-img-watermark": "^1.1.0",
        "boxicons": "^2.1.2",
        "firebase": "^9.8.3",
        "vue-loader": "^17.0.0",
        "vue-router": "^4.0.13",
        "vue-sorted-table": "^1.3.0",
